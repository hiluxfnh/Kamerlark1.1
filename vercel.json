{
  "version": 2,
  "buildCommand": "NEXT_PUBLIC_SKIP_STATIC_GENERATION=true next build --no-lint",
  "devCommand": "next dev",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "src/app/api/**/*": {
      "runtime": "nodejs18.x",
      "memory": 1024
    },
    "src/app/admin/**/*": {
      "runtime": "nodejs18.x",
      "memory": 1024
    }
  }
}
